<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Socially.MobileApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Socially.MobileApp"
    xmlns:page="clr-namespace:Socially.MobileApp.Pages"
    Shell.NavBarIsVisible="False"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate page:InitialPage}"
        Route="MainPage" />

</Shell>
